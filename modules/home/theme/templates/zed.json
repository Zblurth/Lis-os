{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Matugen",
  "author": "Matugen",
  "themes": [
    {
      "name": "Matugen",
      "appearance": "dark",
      "style": {
        "background": "{{colors.surface.default.hex}}",
        "foreground": "{{colors.on_surface.default.hex}}",
        "border": "{{colors.outline.default.hex}}",
        "border.variant": "{{colors.surface_container_highest.default.hex}}",
        "elevated_surface.background": "{{colors.surface_container.default.hex}}",
        "surface.background": "{{colors.surface.default.hex}}",
        "editor.background": "{{colors.surface.default.hex}}",
        "editor.foreground": "{{colors.on_surface.default.hex}}",
        "editor.active_line.background": "{{colors.surface_container_highest.default.hex}}",
        "editor.line_number": "{{colors.on_surface_variant.default.hex}}",
        "editor.active_line_number": "{{colors.primary.default.hex}}",
        "terminal.background": "{{colors.surface.default.hex}}",
        "terminal.ansi.black": "{{colors.surface.default.hex}}",
        "terminal.ansi.red": "{{colors.error.default.hex}}",
        "terminal.ansi.green": "{{colors.primary.default.hex}}",
        "terminal.ansi.yellow": "{{colors.tertiary.default.hex}}",
        "terminal.ansi.blue": "{{colors.secondary.default.hex}}",
        "terminal.ansi.magenta": "{{colors.primary.default.hex}}",
        "terminal.ansi.cyan": "{{colors.secondary.default.hex}}",
        "terminal.ansi.white": "{{colors.on_surface.default.hex}}"
      }
    }
  ]
}
